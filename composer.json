{
    "name": "imritesh/livecrud",
    "description": "Generate Basic Crud Operations With Livewire and Tailwind Css For Laravel",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Ritesh Singh",
            "email": "hello@imritesh.com",
            "homepage": "https://imritesh.com"
        }
    ],
    "homepage": "https://github.com/riteshsingh1/livewire-crud",
    "keywords": ["Laravel", "LiveCrud"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Imritesh\\LiveCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imritesh\\LiveCrud\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imritesh\\LiveCrud\\LiveCrudServiceProvider"
            ],
            "aliases": {
                "LiveCrud": "Imritesh\\LiveCrud\\Facades\\LiveCrud"
            }
        }
    }
}
